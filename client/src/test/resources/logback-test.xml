<?xml version="1.0" encoding="UTF-8"?>
<configuration debug="true">

  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    
    <encoder>
      <pattern>%d{HH:mm:ss.SSS} %-5level [%thread{12}] %logger{36} - %method - %msg%n</pattern>
    </encoder>
  </appender>


	<logger name="jcox.security" level="DEBUG" />

  <root level="INFO">
    <appender-ref ref="STDOUT" />
  </root>
  
</configuration>
